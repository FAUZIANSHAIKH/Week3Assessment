package com.sapient.productservice.model;

import java.io.Serializable;
import javax.xml.bind.annotation.*;


import lombok.Data;

@Data
@XmlRootElement
@XmlAccessorType(XmlAccessType.FIELD)
public class Product  implements Serializable
{
	@XmlElement
	private int id;
	@XmlElement
	private String name;
	@XmlElement
	private double price;
	
}
